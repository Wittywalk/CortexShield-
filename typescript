[?2004l# main.py
from codeecho import CodeEcho
from terminal_mirror_save import TerminalMirrorSave
import time
import subprocess
from android.permissions import request_permissions, Permission

# Request permissions at runtime (only if necessary for Android 6.0+)
request_permissions([Permission.INTERNET, Permission.WRITE_EXTERNAL_STORAGE, Permission.READ_EXTERNAL_STORAGE])

# Initialize both logging tools
codeecho = CodeEcho("aethermind_log.log")
terminal_mirror = TerminalMirrorSave("terminal_output.log")

# Log some key events using CodeEcho
codeecho.run_echo("AetherMind has started.")
time.sleep(1)
codeecho.run_echo("SafeHarbor has been activated.")
time.sleep(1)

# Now, capture terminal output using TerminalMirrorSave
terminal_mirror.capture_terminal_output()

# Example of a long-running process or event to monitor
for i in range(5):
    terminal_mirror.save_output(f"Running process {i+1}...")
    time.sleep(2)  # Simulate some work
    codeecho.run_echo(f"Process {i+1} completed.")

# Run overwatch.py and helm.py as separate background processes
overwatch_process = subprocess.Popen(["python3", "Overwatch.py"])
helm_process = subprocess.Popen(["python3", "Helm.py"])

# Optional: Wait for these processes to finish if needed (this part is optional)
overwatch_process.wait()
helm_process.wait()
