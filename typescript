[?2004h[0;32m~/CortexShield/scripts[0m [0;97m$[0m cat safeharbor.py
[?2004limport os
import time
import subprocess
import threading

class SafeHarbor:
    def __init__(self):
        print("SafeHarbor (Version 1.0) is now active!")

    def check_for_intrusions(self):
        try:
            # Monitor running processes
            print("SafeHarbor: Checking for unusual processes...")
            process = subprocess.Popen(["ps", "aux"], stdout=subprocess.PIPE, stderr=subprocess.PIPE)
            output, error = process.communicate()

            if error:
                print(f"SafeHarbor Error: {error.decode('utf-8')}")
                return

            # Check if there are any unknown or suspicious processes
            processes = output.decode("utf-8").splitlines()
            suspicious_processes = []

            # Let's say a suspicious process has 'python' in its name
            for proc in processes:
                if "python" in proc:  # Modify this as needed to match suspicious activity
                    suspicious_processes.append(proc)

            if suspicious_processes:
                print("Suspicious processes found:")
                for proc in suspicious_processes:
                    print(f"  > {proc}")
            else:
                print("No suspicious processes found.")

            # Monitoring open files (without root access)
            print("SafeHarbor: Checking for unusual open files...")
            process = subprocess.Popen(["lsof", "-i"], stdout=subprocess.PIPE, stderr=subprocess.PIPE)
            output, error = process.communicate()

            if error:
                print(f"SafeHarbor Error: {error.decode('utf-8')}")
                return

            open_files = output.decode("utf-8").splitlines()
            if open_files:
                print("Open files and network connections:")
                for line in open_files:
                    print(f"  > {line}")
            else:
                print("No unusual open files or network connections found.")

        except Exception as e:
            print(f"SafeHarbor Error: {e}")

    def run(self):
        while True:
            self.check_for_intrusions()
            time.sleep(10)  # Check every 10 seconds

def start_safeharbor_in_thread():
    # Running SafeHarbor in a separate thread
    firewall = SafeHarbor()
    thread = threading.Thread(target=firewall.run)
    thread.daemon = True
    thread.start()

if __name__ == "__main__":
    start_safeharbor_in_thread()
