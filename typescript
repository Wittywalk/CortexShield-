#!/bin/bash

cd ~/CortexShield/scripts

python3 run_all_processes.py

# Create a new tmux session named "AetherSafe" and start AetherMind in the same window
tmux new-session -d -s AetherSafe "python3 ~/CortexShield/scripts/aethermind.py; bash"

# Send command to start SafeHarbor in the same window
tmux send-keys -t AetherSafe "python3 ~/CortexShield/scripts/safeharbor.py" C-m

# Send command to start main.py in the same window
tmux send-keys -t AetherSafe "python3 ~/CortexShield/scripts/main.py" C-m

# Send command to start Overwatch in the same window
tmux send-keys -t AetherSafe "python3 ~/CortexShield/scripts/Overwatch.py" C-m

# Send command to start Helm in the same window
tmux send-keys -t AetherSafe "python3 ~/CortexShield/scripts/Helm.py" C-m

# Send command to start TerminalMirrorSave in the same window
tmux send-keys -t AetherSafe "python3 ~/CortexShield/modules/terminalmirrorsave.py" C-m

# Attach to the session so you can monitor all processes
tmux attach-session -t AetherSafe
