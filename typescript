Overwatch.py  requirements.txt
[?2004h[0;32m~/CortexShield/scripts[0m [0;97m$[0m cat Overwatch.py
[?2004l#!/usr/bin/env python3
import os
import time
import shutil
import hashlib
import subprocess
from datetime import datetime

class Overwatch:
    def __init__(self):
        self.name = "Overwatch"
        self.version = "1.0"
        self.file_path = '/root/CortexShield/scripts/aethermind.py'  # Path to the AetherMind script
        self.checksum_file = '/root/CortexShield/scripts/aethermind_checksum.txt'  # Path to the checksum file
        self.backup_dir = '/root/CortexShield/backup'  # Backup directory

    # Function to check file integrity by comparing checksums
    def check_file_integrity(self):
        try:
            with open(self.file_path, 'rb') as file:
                file_data = file.read()
                file_hash = hashlib.sha256(file_data).hexdigest()

            # Check if the checksum matches the saved one
            with open(self.checksum_file, 'r') as f:
                saved_hash = f.read().strip()

            return file_hash == saved_hash
        except Exception as e:
            print(f"Error checking integrity of {self.file_path}: {e}")
            return False

    # Function to perform a backup
    def perform_backup(self):
        try:
            timestamp = datetime.now().strftime("%Y%m%d_%H%M%S")
            backup_name = f"backup_{timestamp}"
            backup_path = os.path.join(self.backup_dir, backup_name)

            # Create a backup
            shutil.copytree(self.file_path, backup_path)
            print(f"Backup created at: {backup_path}")
        except Exception as e:
            print(f"Error creating backup: {e}")

    # Function to monitor system processes and check integrity
    def monitor_system(self):
        while True:
            # Check system integrity
            if not self.check_file_integrity():
                print("Integrity check failed! Restoring from backup...")
                # Restore from backup (optional - can add your own logic here)
                self.perform_backup()

            # Monitor system every 60 seconds
            time.sleep(60)

    # Main function to run Overwatch functionality
    def run(self):
        print(f"{self.name} (Version {self.version}) is now active!")
        self.monitor_system()


if __name__ == "__main__":
    overwatch = Overwatch()
    overwatch.run()
