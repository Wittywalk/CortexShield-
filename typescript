# (String) Title of your application
title = CortexShield

# (String) Package name
package.name = cortexshield
package.domain = org.wittywalk  # Change as needed

# (String) Version of your application
version = 1.0

# (String) Source code where main.py is located
source.dir = .

# (String) The main .py file to use as the entry point
source.main = run_all_processes.py

# (List) Application requirements
requirements = python3,kivy,requests==2.28.1,termcolor==1.1.0,loguru==0.5.3,psutil==5.9.1,simplejson==3.17.6,schedule==1.1.0,cython==0.29.24,pyjnius==1.3.0,pillow==8.4.0,python-for-android==0.7.3,setuptools==56.0.0

# (List) Permissions your app requires
android.permissions = INTERNET,WRITE_EXTERNAL_STORAGE,READ_EXTERNAL_STORAGE,WAKE_LOCK,FOREGROUND_SERVICE,RECEIVE_BOOT_COMPLETED

# (String) Path to the icon image
icon.filename = /storage/emulated/0/Downloads/CortexShield.png

# (List) Files or directories to include in the APK
source.include_exts = py,txt,png,jpg,json,sh
include_patterns = *.py,*.json,*.sh

# (str) Application orientation (portrait, landscape or all)
orientation = portrait

# (bool) Indicate if the application should be fullscreen or not
fullscreen = 0

# (bool) Enable Android logcat
log_enable = 1

# (int) Android API to use
android.api = 33

# (int) Minimum API your APK will support
android.minapi = 21

# (int) Android SDK version to use
android.sdk = 33

# (str) Android NDK version to use
android.ndk = 25b

# (str) Android build tool version
android.build_tools_version = 33.0.2

# (bool) Use --private data storage (True) or --dir public storage (False)
android.private_storage = True

