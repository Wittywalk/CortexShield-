[?2004limport subprocess
import time

# Function to start AetherMind and SafeHarbor processes
def start_processes():
    try:
        print("Starting AetherMind...")
        subprocess.Popen(["python3", "/path/to/aethermind.py"])  # Update with the actual path
        
        print("Starting SafeHarbor...")
        subprocess.Popen(["python3", "/path/to/safeharbor.py"])  # Update with the actual path
    except Exception as e:
        print(f"Error starting processes: {e}")

# Function to stop AetherMind and SafeHarbor processes
def stop_processes():
    try:
        print("Stopping AetherMind...")
        subprocess.run(["pkill", "-f", "aethermind.py"])
        
        print("Stopping SafeHarbor...")
        subprocess.run(["pkill", "-f", "safeharbor.py"])
    except Exception as e:
        print(f"Error stopping processes: {e}")

# Function to perform a strategic action, e.g., start, stop, or configure
def perform_action(action):
    if action == "start":
        start_processes()
    elif action == "stop":
        stop_processes()
    elif action == "status":
        print("Checking system status...")
        # Here you could implement a system status check, like CPU usage or processes running
    else:
        print("Invalid action!")

# Main function to run Helm as a control center
if __name__ == "__main__":
    print("Helm is now running and awaiting your commands...")
    
    while True:
        print("Enter an action (start/stop/status):")
        action = input().strip().lower()
        perform_action(action)
        time.sleep(1)  # Wait for a moment before accepting the next command
